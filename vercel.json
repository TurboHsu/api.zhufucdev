{
  "version": 2,
  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/releases",
      "dest": "/releases"
    },
    {
      "src": "/origins",
      "dest": "/origins"
    }
  ]
}